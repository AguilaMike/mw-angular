{
  "name": "mw-angular",
  "version": "0.0.0",
  "description": "Most Wanted Angular",
  "private": true,
  "author": "Andrey Korovin <misticwonder@gmail.com>",
  "homepage": "https://github.com/misticwonder/mw-angular#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/misticwonder/mw-angular.git"
  },
  "browser": {
    "fs": false
  },
  "scripts": {
    "------------------ site ------------------": "",
    "start:site": "ng serve site --port 4200",
    "start-for-devices:site": "npm run start:site -- --host 0.0.0.0 --ssl",
    "start-with-ssr:site": "./tools/start-complete-app.sh site development",
    "lint:site": "ng lint site",
    "test:site": "ng test site --watch=false --browsers=ChromeHeadless",
    "test-watch:site": "ng test site",
    "e2e:site": "ng e2e site --port 6200",
    "e2e-ci:site": "ng e2e site --port 6200 --protractor-config=./apps/site/e2e/protractor-ci.conf.js",
    "analize:site": "./tools/analize-bundles.sh site development",
    "extract-translations:site": "ngx-translate-extract --input ./apps/site/src --output ./apps/site/src/assets/i18n/{en,ru}.json --clean --sort --format namespaced-json --marker TRANSLATE --format-indentation \"  \"",
    "------------------ core lib ------------------": "",
    "check:core": "./tools/check-lib.sh mw-angular/core",
    "build-lib:core": "./tools/build-lib.sh mw-angular/core",
    "build-lib-watch:core": "./tools/build-lib-watch.sh mw-angular/core",
    "publish-major:core": "./tools/publish-lib.sh mw-angular/core major",
    "publish-minor:core": "./tools/publish-lib.sh mw-angular/core minor",
    "publish-patch:core": "./tools/publish-lib.sh mw-angular/core patch",
    "------------------ complex-filter lib ------------------": "",
    "check:complex-filter": "./tools/check-lib.sh mw-angular/complex-filter",
    "build-lib:complex-filter": "./tools/build-lib.sh mw-angular/complex-filter",
    "build-lib-watch:complex-filter": "./tools/build-lib-watch.sh mw-angular/complex-filter",
    "publish-major:complex-filter": "./tools/publish-lib.sh mw-angular/complex-filter major",
    "publish-minor:complex-filter": "./tools/publish-lib.sh mw-angular/complex-filter minor",
    "publish-patch:complex-filter": "./tools/publish-lib.sh mw-angular/complex-filter patch",
    "------------------ media lib ------------------": "",
    "check:media": "./tools/check-lib.sh mw-angular/media",
    "build-lib:media": "./tools/build-lib.sh mw-angular/media",
    "build-lib-watch:media": "./tools/build-lib-watch.sh mw-angular/media",
    "publish-major:media": "./tools/publish-lib.sh mw-angular/media major",
    "publish-minor:media": "./tools/publish-lib.sh mw-angular/media minor",
    "publish-patch:media": "./tools/publish-lib.sh mw-angular/media patch",
    "------------------ common ------------------": "",
    "build-all-libs": "./tools/build-all-libs.sh",
    "check-all-libs": "./tools/check-all-libs.sh",
    "lint-all-scss": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
    "lint-all-scss-fix": "npm run lint-scss -- --fix",
    "prettier": "prettier --write \"{apps,libs}/**/*.ts\"",
    "prettier:check": "prettier --check \"{apps,libs}/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./tools/husky/commit-msg.js",
      "pre-commit": "pretty-quick --staged",
      "pre-push": "./tools/pre-push.sh"
    }
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/router": "~8.0.0",
    "@nguniversal/common": "^8.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "prettier": "1.17.1",
    "pretty-quick": "^1.11.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "scss-bundle": "^2.5.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "wallaby-webpack": "^3.9.15",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0"
  }
}
