{
  "name": "mw-angular",
  "version": "0.0.0",
  "description": "MW Angular Modules Development Kit",
  "private": true,
  "author": "Andrey Korovin <misticwonder@gmail.com>",
  "homepage": "https://github.com/misticwonder/mw-angular#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/misticwonder/mw-angular.git"
  },
  "scripts": {
    "------------------ site ------------------": "",
    "start:site": "ng serve site --port 4200",
    "start-for-devices:site": "npm run start:site -- --host 0.0.0.0 --ssl",
    "start-with-ssr:site": "./tools/start-complete-app.sh site development",
    "lint:site": "ng lint site",
    "test:site": "ng test site --watch=false --browsers=ChromeHeadless",
    "test-watch:site": "ng test site",
    "e2e:site": "ng run site-e2e:e2e --port 6200",
    "analize:site": "./tools/analize-bundles.sh site development",
    "------------------ core lib ------------------": "",
    "check:core": "./tools/check-lib.sh mw-angular/core",
    "build-lib:core": "./tools/build-lib.sh mw-angular/core",
    "build-lib-watch:core": "./tools/build-lib-watch.sh mw-angular/core",
    "publish:core": "./tools/publish-lib.sh mw-angular/core",
    "------------------ complex-filter lib ------------------": "",
    "check:complex-filter": "./tools/check-lib.sh complex-filter",
    "build-lib:complex-filter": "./tools/build-lib.sh complex-filter",
    "build-lib-watch:complex-filter": "./tools/build-lib-watch.sh complex-filter",
    "publish:complex-filter": "./tools/publish-lib.sh complex-filter",
    "------------------ media lib ------------------": "",
    "check:media": "./tools/check-lib.sh mw-angular/media",
    "build-lib:media": "./tools/build-lib.sh mw-angular/media",
    "build-lib-watch:media": "./tools/build-lib-watch.sh mw-angular/media",
    "publish:media": "./tools/publish-lib.sh mw-angular/media",
    "------------------ common ------------------": "",
    "build-all-libs": "./tools/build-all-libs.sh",
    "check-all-libs": "./tools/check-all-libs.sh",
    "lint-all-scss": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
    "lint-all-scss-fix": "npm run lint-scss -- --fix",
    "prettier": "prettier --write \"{apps,libs}/**/*.ts\"",
    "prettier:check": "prettier --check \"{apps,libs}/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "./tools/pre-push.sh"
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nguniversal/express-engine": "0.0.0",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "^7.2.14",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.48",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~4.5.0",
    "husky": "^2.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "1.17.1",
    "pretty-quick": "^1.11.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "wallaby-webpack": "^3.9.15",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0"
  }
}
