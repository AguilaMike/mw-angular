{
  "name": "mw-angular",
  "version": "0.0.0",
  "scripts": {
    "------------------ sandbox ------------------": "",
    "start-no-libs:sandbox": "ng serve sandbox --port 4200",
    "start-no-libs-for-devices:sandbox": "ng serve sandbox --host 0.0.0.0 --port 4200 --ssl",
    "start:sandbox": "npm run build-libs && npm run start-no-libs:sandbox",
    "start-complete:sandbox": "rimraf dist/sandbox && npm run build:sandbox -- --configuration development && npm run build-ssr:sandbox && npm run webpack:sandbox && npm run start-ssr:sandbox",
    "build:sandbox": "npm run build-libs && ng build sandbox --delete-output-path",
    "build-ssr:sandbox": "ng run sandbox:server --delete-output-path",
    "webpack:sandbox": "webpack --config ./apps/sandbox/webpack.server.config.js --progress --colors && node dist/sandbox/prerender.js",
    "start-ssr:sandbox": "node dist/sandbox/server.js",
    "------------------ env lib ------------------": "",
    "build:env": "rimraf dist/mw-angular/env && ng build @mw-angular/env",
    "build-watch:env": "rimraf dist/mw-angular/env && ng build @mw-angular/env --watch",
    "lint:env": "ng lint @mw-angular/env",
    "test:env": "ng test @mw-angular/env --watch=false --browsers=ChromeHeadless",
    "------------------ complex-filter lib ------------------": "",
    "build:complex-filter": "rimraf dist/mw-angular/complex-filter && ng build @mw-angular/complex-filter",
    "build-watch:complex-filter": "rimraf dist/mw-angular/complex-filter && ng build @mw-angular/complex-filter --watch",
    "lint:complex-filter": "ng lint @mw-angular/complex-filter",
    "test:complex-filter": "ng test @mw-angular/complex-filter --watch=false --browsers=ChromeHeadless",
    "------------------ media lib ------------------": "",
    "build:media": "rimraf dist/mw-angular/media && ng build @mw-angular/media",
    "build-watch:media": "rimraf dist/mw-angular/media && ng build @mw-angular/media --watch",
    "lint:media": "ng lint @mw-angular/media",
    "test:media": "ng test @mw-angular/media --watch=false --browsers=ChromeHeadless",
    "------------------ common ------------------": "",
    "lint-scss": "stylelint \"apps/**/*.scss\" && stylelint \"libs/**/*.scss\"",
    "build-libs": "npm run build:env && npm run build:complex-filter && npm run build:media"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nguniversal/express-engine": "0.0.0",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "^7.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.48",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "wallaby-webpack": "^3.9.15",
    "webpack-cli": "^3.1.0"
  }
}
